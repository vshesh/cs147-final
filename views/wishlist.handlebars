<body>
	{{> menu-partial}}
	{{> menupanel-partial}}
	<div class="container">
		{{#each wishlist}}
		<a href="/info/20">
			<div class="swipe-tile" id="swipe1">
				<div class="wishlist-entry">
					<div class="col-xs-3 photo">
						<img src="http://lorempixel.com/400/400/food">
					</div>
					<div class= "col-xs-6">
						<ul>		
							<li>{{name}}</li>
							<li>{{cuisines}}</li
								>
							<li><span class="glyphicon glyphicon-star"></span><span class="glyphicon glyphicon-star"></span><span class="glyphicon glyphicon-star"></span><span class="glyphicon glyphicon-star"></span><span class="glyphicon glyphicon-star-empty"></span></li>
						</ul>
					</div>
					<div class= "col-xs-3 right-info">
						<ul>
							<li>{{distance}}</li>
							<li>{{hours}}</li>
							<li>{{price}}</li>
							
						</ul>
					</div>
				</div>
			</div>
		</a>
		{{/each}}
		<p><button type="button" id="location">Show my location</button></p>
	<div id="out"></div>
	</div>

		<!-- Req for menubar-->
		<script src="/js/menubar/modernizr.custom.js"></script>
		<!-- Classie - class helper functions by @desandro https://github.com/desandro/classie -->
