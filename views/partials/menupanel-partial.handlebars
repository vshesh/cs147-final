			body = document.body;


		$('#menuButton').click(function() {
				classie.toggle( this, 'active' );
				classie.toggle( body, 'cbp-spmenu-push-toright' );
				classie.toggle( menuLeft, 'cbp-spmenu-open' );
				classie.toggle( menuLeft, 'put-in-shadow' );
			});
		$('.html').click(function(){
			console.log("made it!");
			if($('#menuButton').hasClass('active')){
				classie.toggle( this, 'active' );
				classie.toggle( body, 'cbp-spmenu-push-toright' );
				classie.toggle( menuLeft, 'cbp-spmenu-open' );
				classie.toggle( menuLeft, 'put-in-shadow' );

			}

		});
